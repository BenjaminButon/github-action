{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "deploy": "echo \"Not implemented yet\"",
    "dev": "next dev",
    "generate": "plop",
    "lint": "eslint '*/**/*.{js,ts,tsx,jsx}'",
    "lint-fix": "eslint --fix",
    "predeploy": "npm run lint && npm run test",
    "prepare": "husky install",
    "prettier-format": "prettier --write",
    "start": "next start",
    "stylelint": "stylelint ./src/**/*.{css,scss,sass,less}",
    "stylelint-fix": "stylelint --fix",
    "test": "jest --config=jest.config.js --passWithNoTests"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run prettier-format",
      "npm run lint-fix"
    ],
    "*.{css,scss,sass,less}": [
      "npm run prettier-format",
      "npm run stylelint-fix"
    ]
  },
  "dependencies": {
    "next": "^11.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.35.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@linkup/eslint-config-next-js": "github:LinkUpStudio/eslint-config-next-js.git#v1.0.1",
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.14",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-jest": "^27.0.6",
    "enzyme": "^3.11.0",
    "eslint": "^7.30.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^11.0.1",
    "plop": "^2.7.4",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  }
}
